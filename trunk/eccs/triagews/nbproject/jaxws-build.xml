    <target name="wsgen-service-compile" depends="wsgen-CreatePatientService, wsgen-SendRxDataService, wsgen-CreateCaseService, wsgen-ViewCaseService, wsgen-SearchService, wsgen-ReferCaseService, wsgen-ViewPatientService, wsgen-ViewUserService, wsgen-ViewHospitalService, wsgen-EditUserService, wsgen-RegisterUserService">
        <webproject2:javac srcdir="${build.generated.dir}/wsgen/service" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${build.classes.dir}"/>
    </target>
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${java.home}/../lib/tools.jar:${j2ee.platform.wsimport.classpath}:${javac.classpath}"/>
        </taskdef>
        <condition property="conf-dir" value="${conf.dir}/" else="">
            <isset property="conf.dir"/>
        </condition>
    </target>
    <target name="wsimport-client-check-DocUserService" depends="wsimport-init">
        <condition property="wsimport-client-DocUserService.notRequired">
            <available file="${build.generated.dir}/wsimport/client/ws" type="dir"/>
        </condition>
    </target>
    <target name="wsimport-client-DocUserService" depends="wsimport-init,wsimport-client-check-DocUserService" unless="wsimport-client-DocUserService.notRequired">
        <wsimport xendorsed="true" verbose="true" sourcedestdir="${build.generated.dir}/wsimport/client" extension="true" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${conf-dir}xml-resources/web-service-references/DocUserService/wsdl/localhost_15880/PagsanjanUser/DocUserService.wsdl" wsdlLocation="http://localhost:15880/PagsanjanUser/DocUserService?wsdl" catalog="catalog.xml"/>
        <copy todir="${build.classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/binaries" includes="**/*.xml"/>
        </copy>
    </target>
    <target name="wsimport-client-clean-DocUserService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/ws"/>
    </target>
    <target name="wsimport-client-check-SendRxDataService" depends="wsimport-init">
        <condition property="wsimport-client-SendRxDataService.notRequired">
            <available file="${build.generated.dir}/wsimport/client/ws" type="dir"/>
        </condition>
    </target>
    <target name="wsimport-client-SendRxDataService" depends="wsimport-init,wsimport-client-check-SendRxDataService" unless="wsimport-client-SendRxDataService.notRequired">
        <wsimport xendorsed="true" verbose="true" sourcedestdir="${build.generated.dir}/wsimport/client" extension="true" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${conf-dir}xml-resources/web-service-references/SendRxDataService/wsdl/localhost_8080/Pagsanjan/SendRxDataService.wsdl" wsdlLocation="http://localhost:8080/Pagsanjan/SendRxDataService?wsdl" catalog="catalog.xml"/>
        <copy todir="${build.classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/binaries" includes="**/*.xml"/>
        </copy>
    </target>
    <target name="wsimport-client-clean-SendRxDataService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/ws"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-DocUserService, wsimport-client-SendRxDataService"/>
    <target name="wsimport-client-compile" depends="-pre-pre-compile">
        <webproject2:javac srcdir="${build.generated.dir}/wsimport/client" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${build.classes.dir}"/>
    </target>
</project>
